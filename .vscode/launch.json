    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Users Service",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {
                "POSTGRES_HOST": "localhost",
                "POSTGRES_DB": "microservice",
                "POSTGRES_USER": "microservice",
                "POSTGRES_PASSWORD": "example123"
